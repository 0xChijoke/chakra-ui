{
  "name": "@chakra-ui/accordion",
  "version": "1.3.8",
  "description": "A simple and accessible accordion component for React & Chakra UI",
  "keywords": [
    "react",
    "component",
    "accordion",
    "chakra",
    "chakra ui",
    "collapse",
    "accessible",
    "accessibility",
    "wai-aria",
    "aria",
    "a11y"
  ],
  "author": "Segun Adebayo <sage@adebayosegun.com>",
  "homepage": "https://github.com/chakra-ui/chakra-ui#readme",
  "license": "MIT",
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.module.js",
  "types": "dist/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chakra-ui/chakra-ui.git",
    "directory": "packages/accordion"
  },
  "bugs": {
    "url": "https://github.com/chakra-ui/chakra-ui/issues"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "start": "microbundle watch --format modern",
    "build": "microbundle --jsx React.createElement --jsxFragment React.Fragment --format modern,esm,cjs",
    "test": "jest --env=jsdom --passWithNoTests",
    "lint": "concurrently yarn:lint:*",
    "version": "yarn build",
    "test:cov": "yarn test --coverage",
    "lint:src": "eslint src --ext .ts,.tsx --config ../../.eslintrc",
    "lint:types": "tsc --noEmit"
  },
  "dependencies": {
    "@chakra-ui/descendant": "2.0.1",
    "@chakra-ui/hooks": "1.6.1",
    "@chakra-ui/icon": "1.1.12",
    "@chakra-ui/react-utils": "1.1.2",
    "@chakra-ui/transition": "1.3.6",
    "@chakra-ui/utils": "1.8.3"
  },
  "devDependencies": {
    "@chakra-ui/system": "1.7.4",
    "react": "^17.0.1"
  },
  "peerDependencies": {
    "@chakra-ui/system": ">=1.0.0",
    "react": ">=16.8.6"
  }
}
